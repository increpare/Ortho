./clean

cd c
make
cd ..

for fn in dat/json/*.json
do
	echo "processing $fn"
	./json2png -noGridLines $fn
	./json2svg -noGridLines $fn
	./json2bin $fn
	./retop $fn
done

for fn in dat/retop/*.json
do
	echo "processing $fn"
	./json2png -diagonals -selectiveGridLines $fn
	./json2svg -diagonals -selectiveGridLines $fn
	./json2bin $fn
done

./testbin
