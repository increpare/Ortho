<?xml version='1.0' encoding='utf-8'?>
<widget id="com.increpare.orthodocs" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>orthodocs</name>
    <description>
        Documentation for the Orthona programming language
    </description>
    <author email="analytic@gmail.com" href="http://www.increpare.com">
        Stephen Lavelle
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
